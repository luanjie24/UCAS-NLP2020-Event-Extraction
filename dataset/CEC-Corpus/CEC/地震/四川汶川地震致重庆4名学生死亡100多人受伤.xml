	<eRelation relType="Causal" cause_eid="e6" effect_eid="e8"/>  
	<eRelation relType="Causal" cause_eid="e3" effect_eid="e9"/>  
	<eRelation relType="Causal" cause_eid="e9" effect_eid="e10"/>  
	<eRelation relType="Causal" cause_eid="e9" effect_eid="e11"/>  
	<eRelation relType="Causal" cause_eid="e9" effect_eid="e12"/>  
