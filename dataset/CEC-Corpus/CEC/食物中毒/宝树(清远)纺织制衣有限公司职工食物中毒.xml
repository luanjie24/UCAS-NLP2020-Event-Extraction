<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>宝树(清远)纺织制衣有限公司职工食物中毒</Title>  
	<ReportTime type="absTime">2009年3月21日</ReportTime>  
	<Content> 
		<Paragraph>
			<Sentence>
				<Event eid="e1">
					<Time tid="t1" type="relTime">3月20日</Time>,宝树(清远)纺织制衣有限公司
					<Participant sid="s1">17名员工</Participant>在
					<Location lid="l1">该食堂</Location>
					<Denoter did="d1" type="action">进食</Denoter>
					<Object oid="o1">晚餐</Object>后
				</Event>,
				<Event eid="e2">陆续
					<Participant sid="s2">有人</Participant>
					<Denoter did="d2" type="stateChange">出现</Denoter>呕吐、腹痛、腹泻等
					<Object oid="o2">食物中毒症状</Object>
				</Event>,
				<Event eid="e3">于
					<Time tid="t3" type="relTime">3月21日</Time>早上陆续
					<Denoter did="d3" type="movement">前往</Denoter>
					<Location lid="l3">广清医院</Location>
				</Event>
				<Event eid="e4">
					<Denoter did="d4" type="operation">就诊</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e5">
					<Participant sid="s5">市卫生局</Participant>
					<Denoter did="d5" type="action">接报</Denoter>
				</Event>后
				<Event eid="e6">立即
					<Denoter did="d6" type="operation">组织</Denoter>
					<Participant sid="s6">市疾控中心</Participant>、
					<Participant sid="s6">市卫生监督所</Participant>
				</Event>
				<Event eid="e7">
					<Denoter did="d7" type="movement">赴</Denoter>
					<Location lid="l7">现场</Location>
				</Event>
				<Event eid="e8">
					<Denoter did="d8" type="operation">调查</Denoter>
				</Event>,
				<Event eid="e9">经流行病学
					<Denoter did="d9" type="operation">调查</Denoter>
				</Event>及
				<Event eid="e10">
					<Participant sid="s10">实验室</Participant>
					<Denoter did="d10" type="operation">检验</Denoter>
				</Event>,
				<Event eid="e11">
					<Denoter did="d11" type="perception">认定</Denoter>是一起细菌性食物中毒事件
				</Event>。
			</Sentence>
		</Paragraph>  
		<eRelation relType="Causal" cause_eid="e1" effect_eid="e2"/>  
		<eRelation relType="Follow" bevent_eid="e2" aevent_eid="e3"/>  
		<eRelation relType="Accompany" accompanya_eid="e3" accompanyb_eid="e4"/>  
		<eRelation relType="Follow" bevent_eid="e5" aevent_eid="e6"/>  
		<eRelation relType="Follow" bevent_eid="e6" aevent_eid="e7"/>  
		<eRelation relType="Accompany" accompanya_eid="e8" accompanyb_eid="e9"/>  
		<eRelation relType="Follow" bevent_eid="e9" aevent_eid="e10"/> 
	</Content> 
</Body>
