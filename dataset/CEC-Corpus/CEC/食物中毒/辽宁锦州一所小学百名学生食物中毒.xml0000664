<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>辽宁锦州一所小学百名学生食物中毒</Title>  
	<ReportTime type="absTime">2009-04-01</ReportTime>  
	<Content> 
		<Paragraph>
			<Sentence>
				<Event eid="e1">
					<Time tid="t1" type="relTime">3月30日</Time>，
					<Location lid="l1">辽宁省锦州市凌海市班吉塔镇小学</Location>
					<Participant sid="s1,s2">上百名学生</Participant>在
					<Denoter did="d1" type="action">食用</Denoter>学校超市内出售的
					<Object oid="o1">一种袋装食品后</Object>
				</Event>
				<Event eid="e2">集体
					<Denoter did="d2" type="stateChange">出现</Denoter>
					<Object oid="o2">食物中毒现象</Object>
				</Event>，
				<Event eid="e3">
					<Participant sid="s3" type="agent">学校</Participant>及时将
					<Participant oid="o3" type="recipient">出现症状学生</Participant>
					<Denoter did="d3" type="movement">送往</Denoter>
					<Location lid="l3" type="destination">医院</Location>
				</Event>
				<Event eid="e4">
					<Denoter did="d4" type="operation">治疗</Denoter>
				</Event>。
			</Sentence>
		</Paragraph> 
	</Content>  
	<eRelation relType="Causal" cause_eid="e1" effect_eid="e2"/>  
	<eRelation relType="Follow" aevent_eid="e2" bevent_eid="e3"/>  
	<eRelation relType="Follow" aevent_eid="e3" bevent_eid="e4"/> 
</Body>
