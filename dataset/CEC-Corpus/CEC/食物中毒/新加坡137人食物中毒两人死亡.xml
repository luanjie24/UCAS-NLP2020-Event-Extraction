	<eRelation relType="Causal" cause_eid="e4" effect_eid="e5"/>  
	<eRelation relType="Causal" cause_eid="e7" effect_eid="e8"/>  
	<eRelation relType="Causal" cause_eid="e8" effect_eid="e9"/>  
	<eRelation relType="Causal" cause_eid="e10" effect_eid="e11"/>  
	<eRelation relType="Follow" aevent_eid="e13" bevent_eid="e12"/>  
	<eRelation relType="Accompany" accompanyb_eid="e16" accompanya_eid="e17"/> 
</Body>
