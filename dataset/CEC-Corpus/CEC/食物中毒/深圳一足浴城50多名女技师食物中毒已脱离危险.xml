	<eRelation relType="Follow" aevent_eid="e8" bevent_eid="e7"/>  
	<eRelation relType="Follow" aevent_eid="e10" bevent_eid="e9"/>  
	<eRelation relType="Follow" aevent_eid="e12" bevent_eid="e11"/>  
	<eRelation relType="Accompany" accompanyb_eid="e12" accompanya_eid="e13"/> 
</Body>
