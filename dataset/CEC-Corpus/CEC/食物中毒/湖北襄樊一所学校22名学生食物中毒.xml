<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>湖北襄樊一所学校22名学生食物中毒</Title>  
	<ReportTime type="absTime">2008年10月19日</ReportTime>  
	<Content> 
		<Paragraph>
			<Sentence>
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">荆楚网-楚天都市报</Participant>
					<Denoter did="d1" type="statement">信息</Denoter>
				</Event>：
			</Sentence>  
			<Sentence>
				<Event eid="e2">
					<Time tid="t2" type="relTime">10月17日下午</Time>，
					<Location lid="l2">襄樊市襄阳区东津镇一小学</Location>的
					<Participant sid="s2">学生们</Participant>在学校食堂
					<Denoter did="d2" type="action">吃饭</Denoter>后
				</Event>，
				<Event eid="e3">
					<Denoter did="d3" type="stateChange">出现</Denoter>腹痛、恶心等
					<Object oid="o3">食物中毒症状</Object>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e4">经紧急
					<Denoter did="d4" type="action">处理</Denoter>
				</Event>，
				<Event eid="e5">
					<Time tid="t5" type="timeInterval">至昨晚</Time>，
					<Participant sid="s5">中毒学生</Participant>均已
					<Denoter did="d5" type="operation">出院</Denoter>
				</Event>。
			</Sentence>
		</Paragraph> 　　 
		<Paragraph>
			<Sentence>
				<Event eid="e6">
					<Time tid="t6" type="relTime">当晚10时许</Time>，
					<Participant sid="s6">记者</Participant>
					<Denoter did="d6" type="movement">赶到</Denoter>
					<Location lid="l6" type="destination">医院</Location>
				</Event>，
				<Event eid="e7">
					<Participant sid="s7" type="agent">医生</Participant>正对
					<Participant oid="o7" type="recipient">10多名小学生</Participant>进行
					<Denoter did="d7" type="operation">治疗</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event type="thoughtevent" eid="e8">
					<Participant sid="s8">一女生</Participant>
					<Denoter did="d8" type="statement">说</Denoter>
				</Event>，
				<Event eid="e9">
					<Time tid="t9" type="relTime">当天下午</Time>刚上课，
					<Participant sid="s9">有两同学</Participant>
					<Denoter did="d9" type="action">喊</Denoter>肚子疼
				</Event>
				<Event eid="e10">
					<Denoter did="d10" type="movement">离开</Denoter>
					<Location lid="l10">教室</Location>
				</Event>，
				<Event eid="e11">
					<Time tid="t11" type="relTime">不久</Time>，
					<Participant sid="s11">很多同学</Participant>都
					<Denoter did="d11" type="action">喊</Denoter>肚子疼
				</Event>，
				<Event eid="e12">
					<Participant sid="s12" type="agent">学校</Participant>立即将
					<Participant oid="o12" type="recipient">他们</Participant>
					<Denoter did="d12" type="movement">带往</Denoter>
					<Location lid="l12" type="destination">校医务室和镇卫生院</Location>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e13">
					<Participant sid="s13">襄阳区委区政府</Participant>
					<Denoter did="d13" type="perception">闻讯</Denoter>
				</Event>，
				<Event eid="e14">迅速将
					<Participant sid="s14">学生们</Participant>
					<Denoter did="d14" type="movement">转到</Denoter>
					<Location lid="l14" type="destination">市内大医院</Location>
				</Event>，
				<Event eid="e15">
					<Denoter did="d15" type="operation">调集</Denoter>
					<Participant oid="o15">专家</Participant>
				</Event>
				<Event eid="e16">
					<Denoter did="d16" type="operation">会诊治疗</Denoter>
				</Event>。
			</Sentence>
		</Paragraph> 　　 
		<Paragraph>
			<Sentence>
				<Event eid="e17">该
					<Object oid="o17">校名</Object>
					<Denoter did="d17" type="statement">叫</Denoter>一社小学
				</Event>，
				<Event eid="e18">共
					<Denoter did="d18" type="statement">有</Denoter>
					<Participant sid="s18">150余名学生</Participant>
				</Event>，
				<Event eid="e19">
					<Participant sid="s19">学生们</Participant>均
					<Denoter did="d19" type="statement">要求</Denoter>
					<Location lid="l19">在校</Location>吃住
				</Event>。
			</Sentence>
			<Sentence>
				<Event type="thoughtevent" eid="e20">据
					<Denoter did="d20" type="statement">了解</Denoter>
				</Event>，
				<Event eid="e21">
					<Participant sid="s21">中毒学生</Participant>
					<Time tid="t21" type="relTime">当日中午</Time>都
					<Denoter did="d21" type="action">吃了</Denoter>
					<Object oid="o21">食堂的“白菜煮鸭子”</Object>
				</Event>。
			</Sentence>
			<Sentence>
				<Event type="thoughtevent" eid="e22">
					<Participant sid="s22">他们</Participant>
					<Denoter did="d22" type="statement">称</Denoter>
				</Event>，
				<Event eid="e23">
					<Denoter did="d23" type="action">吃</Denoter>
					<Object oid="o23">鸭子</Object>时
				</Event>，
				<Event eid="e24">就已
					<Denoter did="d24" type="perception">闻</Denoter>到
					<Object oid="o24">臭味</Object>
				</Event>。
			</Sentence>
		</Paragraph> 　　 
		<Paragraph>
			<Sentence>
				<Event type="thoughtevent" eid="e25">
					<Participant sid="s25">襄阳区委宣传部</Participant>
					<Denoter did="d25" type="statement">称</Denoter>
				</Event>，中毒学生共22人，
				<Event eid="e26">
					<Participant sid="s26">该校</Participant>
					<Time tid="t26" type="relTime">13日</Time>
					<Denoter did="d26" type="action">买</Denoter>的
					<Object oid="o26">鸭子</Object>
				</Event>，
				<Event eid="e27">连续
					<Denoter did="d27" type="action">吃</Denoter>了
					<Time tid="t27" type="relTime">数天</Time>
				</Event>，
				<Event eid="e28">
					<Time tid="t28" type="relTime">前几日</Time>都没
					<Denoter did="d28" type="stateChange" polarity="negative">出现</Denoter>问题
				</Event>，
				<Event eid="e29">经
					<Participant sid="s29">公安&gt;和卫生部门</Participant>
					<Denoter did="d29" type="operation">调查</Denoter>
				</Event>
				<Event eid="e30">
					<Denoter did="d30" type="perception">发现</Denoter>
				</Event>，
				<Event eid="e31">
					<Object oid="o31">冰箱</Object>出了
					<Denoter did="d31" type="stateChange">故障</Denoter>
				</Event>，
				<Event eid="e32">
					<Object oid="o32">鸭子</Object>已
					<Denoter did="d32" type="stateChange">腐败变质</Denoter>
				</Event>。
			</Sentence>
		</Paragraph>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e32"/>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e8" thoughtcontent_eids="e9-e12"/>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e20" thoughtcontent_eids="e21-e24"/>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e25" thoughtcontent_eids="e26-e32"/>  
		<eRelation relType="Follow" aevent_eid="e2" bevent_eid="e3"/>  
		<eRelation relType="Causal" cause_eid="e4" effect_eid="e5"/>  
		<eRelation relType="Causal" cause_eid="e9" effect_eid="e10"/>  
		<eRelation relType="Causal" cause_eid="e11" effect_eid="e12"/>  
		<eRelation relType="Follow" aevent_eid="e13" bevent_eid="e14"/>  
		<eRelation relType="Accompany" accompanyb_eid="e15" accompanya_eid="e16"/>  
		<eRelation relType="Causal" cause_eid="e31" effect_eid="e32"/> 
	</Content> 
</Body>
