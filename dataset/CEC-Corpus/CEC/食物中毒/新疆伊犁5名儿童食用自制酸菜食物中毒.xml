	<eRelation relType="Follow" aevent_eid="e5" bevent_eid="e6"/>  
	<eRelation relType="Causal" cause_eid="e8" effect_eid="e9"/>  
	<eRelation relType="Follow" aevent_eid="e11" bevent_eid="e12"/>  
	<eRelation relType="Accompany" accompanyb_eid="e12" accompanya_eid="e13"/>  
	<eRelation relType="Causal" cause_eid="e13" effect_eid="e14"/> 
