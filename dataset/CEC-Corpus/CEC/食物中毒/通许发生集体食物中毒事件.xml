	<eRelation relType="Thoughtcontent" thoughtevent_eid="e9" thoughtcontent_eids="e10-e17"/>  
	<eRelation relType="Follow" bevent_eid="e12" aevent_eid="e13"/>  
	<eRelation relType="Follow" bevent_eid="e15" aevent_eid="e16"/> 
