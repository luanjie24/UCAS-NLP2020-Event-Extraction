<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>龙岗一小食店发生一起疑似中毒事件</Title>  
	<ReportTime type="absTime">2008年2月23日</ReportTime>  
	<Content> 
		<Paragraph>
			<Sentence>
				<Event eid="e1">
					<Time tid="t1" type="absTime">2008年2月23日11时左右</Time>，
					<Location lid="l1">龙岗区宝龙工业区一间小食店</Location>发生一起
					<Denoter did="d1" type="emergency">疑似中毒</Denoter>事件
				</Event>，
				<Event eid="e2">附近工厂共有
					<Participant sid="s2,s3">63名员工</Participant>
					<Denoter did="d2" type="stateChange">出现</Denoter>
					<Object oid="o2">疑似中毒症状</Object>
				</Event>
				<Event eid="e3">被
					<Denoter did="d3" type="operation">送院治疗</Denoter>
				</Event>，
				<Event eid="e4">其中
					<Participant sid="s4">2人</Participant>经
					<Denoter did="d4" type="action">抢救无效</Denoter>
				</Event>
				<Event eid="e5">
					<Denoter did="d5" type="stateChange">死亡</Denoter>
				</Event>。
			</Sentence>
			<Sentence>经
				<Event eid="e6">
					<Participant sid="s6">医护人员</Participant>的
					<Denoter did="d6" type="operation">全力救治</Denoter>
				</Event>，
				<Event eid="e7">
					<Time tid="t7" type="timeInterval">截至２４日１５时</Time>，所有患者病情稳定，已有
					<Participant sid="s7">４２人</Participant>
					<Denoter did="d7" type="operation">出院</Denoter>
				</Event>，
				<Event eid="e8">
					<Participant sid="s8">１９人</Participant>
					<Denoter did="d8" type="operation">留院治疗</Denoter>
				</Event>，没有新增死亡病例。
			</Sentence>
			<Sentence>
				<Event eid="e9">此事已被正式
					<Denoter did="d9" type="statement">确认</Denoter>为
					<Object oid="o9">亚硝酸盐中毒事件</Object>
				</Event>。
			</Sentence>
		</Paragraph> 
	</Content>  
	<eRelation relType="Follow" aevent_eid="e1" bevent_eid="e2"/>  
	<eRelation relType="Causal" cause_eid="e2" effect_eid="e3"/>  
	<eRelation relType="Follow" aevent_eid="e3" bevent_eid="e4"/>  
	<eRelation relType="Follow" aevent_eid="e5" bevent_eid="e4"/>  
	<eRelation relType="Accompany" accompanyb_eid="e7" accompanya_eid="e8"/> 
</Body>
