<?xml version="1.0" encoding="UTF-8"?>

<Body>
	<Title>广西贵港一涂料厂溶剂油罐发生火灾 无人员伤亡</Title>  
	<ReportTime type="absTime">2014年04月30日 14:40</ReportTime>  
	<Content>
		<Paragraph>
			<Sentence>
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">中国警察网</Participant>
					<Denoter did="d1" type="statement">讯</Denoter>
				</Event>  
				<Event eid="e2">
					<Time tid="t2" type="relTime">4月29日20时21分</Time>，
					<Location lid="l2">广西贵港市西江农场一涂料厂</Location>
					<Object sid="s2">溶剂油罐</Object>发生
					<Denoter did="d2" type="emergency">火灾</Denoter>
				</Event>， 
				<Event eid="e3">
					<Object sid="s3">大火</Object>
					<Denoter did="d3" type="stateChange">燃烧</Denoter>
				</Event>产生
				<Event eid="e4">大量滚滚
					<Denoter did="d4" type="emergency">浓烟</Denoter>腾
				</Event>，
				<Event eid="e5">
					<Location lid="l5">四周</Location>毗邻
					<Object oid="o5">厂房</Object>、
					<Object oid="o5">房屋</Object>受到
					<Denoter did="d5" type="operation">威胁</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e6">
					<Participant sid="s6-s7">贵港市消防支队指挥中心</Participant>
					<Denoter did="d6" type="perception">接警</Denoter>
				</Event>后，
				<Event eid="e7">迅速
					<Denoter did="d7" type="operation">调集</Denoter>特勤、港北、港南、覃塘4个中队
					<Object oid="o7" sid="s8-s9">11辆消防车</Object>
					<Participant oid="o7" sid="s8-s9">62名官兵</Participant>
				</Event>
				<Event eid="e8">
					<Denoter did="d8" type="movement">前往</Denoter>
				</Event>
				<Event eid="e9">
					<Denoter did="d9" type="operation">扑救</Denoter>
				</Event>。
				<Event eid="e10">
					<Time tid="t10" type="relTime">21时50分</Time>
					<Object oid="o10">大火</Object>被成功
					<Denoter did="d10" type="operation">扑灭</Denoter>
				</Event>，
				<Event eid="e11" polarity="negative">未造成
					<Participant sid="s11">人员</Participant>
					<Denoter did="d11" type="emergency">伤亡</Denoter>
				</Event>。
			</Sentence>
		</Paragraph>
	</Content>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e11"/>  
	<eRelation relType="Accompany" accompanya_eid="e2" accompanyb_eid="e3"/>  
	<eRelation relType="Accompany" accompanya_eid="e3" accompanyb_eid="e4"/>  
	<eRelation relType="Follow" bevent_eid="e6" aevent_eid="e7"/>  
	<eRelation relType="Follow" bevent_eid="e7" aevent_eid="e8"/>  
	<eRelation relType="Follow" bevent_eid="e8" aevent_eid="e9"/>  
	<eRelation relType="Follow" bevent_eid="e9" aevent_eid="e10"/> 
</Body>
