<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>兰州火灾事故</Title>  
	<ReportTime type="absTime">2014年1月2日</ReportTime>  
	<Content>
		<Paragraph> 
			<Sentence> 
				<Event eid="e1"> 
					<Time type="absTime">2014年1月2日</Time>， 
					<Location lid="l1">兰州</Location>  
					<Participant sid="s1">消防官兵</Participant>在紧急 
					<Denoter type="operation" did="d1">处置</Denoter> 
				</Event>  
				<Event eid="e2"> 
					<Object oid="o2">泰生大厦</Object>  
					<Location type="destination" lid="l2">顶部</Location>  
					<Denoter type="emergency" did="d2">火灾</Denoter>。
				</Event> 
			</Sentence>  
			<Sentence> 
				<Event eid="e3"> 
					<Time type="relTime" tid="t3">当日14时20分许</Time>，位于 
					<Location lid="l3">甘肃省兰州市张掖路的泰生大厦顶部</Location>发生 
					<Denoter did="d3" type="emergency">火灾</Denoter>。
				</Event> 
			</Sentence>  
			<Sentence> 
				<Event eid="e4"> 
					<Participant sid="s4">兰州消防</Participant>共 
					<Denoter did="d4" type="operation">调集</Denoter>  
					<Participant oid="o4,s5,s6">6个中队，21台消防车，近200名消防官兵</Participant> 
				</Event>  
				<Event eid="e5"> 
					<Denoter type="movement" did="d5">前往</Denoter> 
				</Event>  
				<Event eid="e6"> 
					<Denoter type="emergency" did="d6">救火</Denoter>。
				</Event> 
			</Sentence>  
			<Sentence>经过2个小时的紧张扑救，火灾于16时40分许扑灭。</Sentence>  
			<Sentence> 
				<Event eid="e7" type="thoughtevent">据 
					<Denoter type="statement" did="d7">了解</Denoter>，
				</Event>  
				<Event eid="e8" polarity="negative">过火面积约1000平方米， 
					<Object sid="s8">大火</Object>没有造成 
					<Participant oid="o8">人员</Participant>  
					<Denoter did="d8" type="emergency">伤亡</Denoter>。
				</Event> 
			</Sentence> 
		</Paragraph> 
	</Content>  
	<eRelation relType="Follow" bevent_eid="e3" aevent_eid="e4"/>  
	<eRelation relType="Composite" fevent_eid="e4" sevent_eids="e5-e6"/>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e7" thoughtcontent_eids="e8"/> 
</Body>
