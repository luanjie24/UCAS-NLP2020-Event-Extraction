	<eRelation relType="Causal" cause_eid="e10" effect_eid="e11"/>  
	<eRelation relType="Follow" bevent_eid="e13" aevent_eid="e14"/>  
	<eRelation relType="Accompany" accompanya_eid="e15" accompanyb_eid="e16"/> 
</Body>
