	<eRelation relType="Follow" bevent_eid="e8" aevent_eid="e9"/>  
	<eRelation relType="Follow" bevent_eid="e9" aevent_eid="e10"/>  
	<eRelation relType="Follow" bevent_eid="e10" aevent_eid="e11"/>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e14" thoughtcontent_eids="e15-e16"/>  
	<eRelation relType="Causal" cause_eid="e16" effect_eid="e15"/> 
</Body>
