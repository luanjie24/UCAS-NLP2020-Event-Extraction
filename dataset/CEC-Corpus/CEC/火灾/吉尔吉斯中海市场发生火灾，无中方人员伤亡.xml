<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>吉尔吉斯中海市场发生火灾，无中方人员伤亡</Title>  
	<ReportTime type="absTime">2008/04/08</ReportTime>  
	<Content> 
		<Paragraph>
			<Sentence>
				<Event eid="e1">
					<Time tid="t1" type="absTime">2008年4月5日上午10时</Time>，
					<Location lid="l1">吉尔吉斯斯坦比什凯克市中国商人集中的中海市场</Location>发生
					<Denoter did="d1" type="emergency">大火</Denoter>
				</Event>，
				<Event eid="e2">
					<Time tid="t2" type="timeInterval">至下午2时</Time>
					<Location lid="l2">火场</Location>
					<Object oid="o2">明火</Object>基本
					<Denoter did="d2" type="stateChange">扑灭</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e3">在
					<Denoter did="d3" type="emergency">火灾</Denoter>中
				</Event>，
				<Event eid="e4">市场售货大楼
					<Object oid="o4">内部设施</Object>及所存
					<Object oid="o4">商品</Object>被
					<Denoter did="d4" type="stateChange">烧毁</Denoter>
				</Event>，
				<Event eid="e5">
					<Object sid="s5">大楼主体结构</Object>
					<Denoter did="d5" type="stateChange">受损</Denoter>严重
				</Event>，
				<Event polarity="negative" eid="e6">所幸无
					<Participant sid="s6">人员</Participant>
					<Denoter did="d6" type="stateChange">伤亡</Denoter>
				</Event>。
			</Sentence>
		</Paragraph>  
		<Paragraph>
			<Sentence>
				<Event eid="e7">
					<Denoter type="perception" did="d7">获悉</Denoter>此事
				</Event>后，
				<Event eid="e8">
					<Participant sid="s8">中国驻吉使馆</Participant>迅速
					<Denoter did="d8" type="operation">启动</Denoter>
					<Object oid="o8">应急机制</Object>
				</Event>，
				<Event eid="e9">
					<Participant sid="s9,s10">张延年</Participant>大使
					<Denoter did="d9" type="movement">亲赴</Denoter>
					<Location lid="l9" type="destination">现场</Location>
				</Event>，
				<Event eid="e10">
					<Denoter did="d10" type="operation">敦促</Denoter>
					<Object oid="o10">吉方</Object>加强对中方商铺的保护
				</Event>，
				<Event eid="e11">
					<Denoter did="d11" type="stateChange">避免</Denoter>灾后中国商户商品被偷、被抢现象的发生
				</Event>；
			</Sentence>
			<Sentence>
				<Event eid="e12">
					<Participant sid="s12,s13,s14">张大使</Participant>还
					<Denoter did="d12" type="operation">慰问</Denoter>了市场内的
					<Participant oid="o12">中国商户</Participant>
				</Event>，
				<Event eid="e13">
					<Denoter did="d13" type="statement">表示</Denoter>
				</Event>
				<Event eid="e14">将会尽力
					<Denoter did="d14" type="operation">协助</Denoter>
					<Participant oid="o14" sid="s15">商户</Participant>
				</Event>
				<Event eid="e15">处理好
					<Denoter did="d15" type="operation">善后工作</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e16">
					<Time type="relTime" tid="t16">目前</Time>
					<Object oid="o16">火灾原因</Object>正在
					<Denoter did="d16" type="operation">调查</Denoter>中
				</Event>。
			</Sentence>
		</Paragraph> 
	</Content>  
	<eRelation relType="Causal" cause_eid="e3" effect_eid="e4"/>  
	<eRelation relType="Causal" cause_eid="e3" effect_eid="e5"/>  
	<eRelation relType="Follow" bevent_eid="e7" aevent_eid="e8"/>  
	<eRelation relType="Causal" cause_eid="e10" effect_eid="e11"/>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e13" thoughtcontent_eids="e14-e15"/>  
	<eRelation relType="Accompany" accompanya_eid="e14" accompanyb_eid="e15"/> 
</Body>
