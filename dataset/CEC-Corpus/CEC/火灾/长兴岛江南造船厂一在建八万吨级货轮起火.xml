<?xml version="1.0" encoding="UTF-8"?>

<Body>
	<Title>长兴岛江南造船厂一在建八万吨级货轮起火</Title>  
	<ReportTime type="absTime">2014年04月09日15:32</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">东方网记者刘歆</Participant>
					<Time tid="t1" type="relTime">4月9日</Time>
					<Denoter did="d1" type="statement">报道</Denoter>
				</Event>： 据
				<Event type="thoughtevent" eid="e2">
					<Object sid="s2">上海市消防局官方微博</Object>
					<Denoter did="d2" type="statement">消息</Denoter>
				</Event>， 
				<Event eid="e3">
					<Time tid="t3" type="relTime">今天中午11时14分</Time>，
					<Object sid="s3">上海市应急联动中心</Object>
					<Denoter did="d3" type="operation">接警</Denoter>
				</Event>， 
				<Event eid="e4">位于
					<Location lid="l4">长兴岛的江南造船厂3号船坞</Location>
					<Object sid="s4">一在建的八万吨级货轮</Object>发生
					<Denoter did="d4" type="emergency">火灾</Denoter>
				</Event>。
			</Sentence>  
			<Sentence>由于 
				<Event eid="e5">
					<Denoter did="d5" type="stateChange">燃烧</Denoter>的主要是
					<Object sid="s5">保温材料</Object>
				</Event>， 
				<Event eid="e6">
					<Location lid="l6">现场</Location>
					<Denoter did="d6" type="emergency">烟雾</Denoter>很大
				</Event>，扑救十分困难。 
				<Event eid="e7">
					<Participant sid="s7">消防部门</Participant>
					<Denoter did="d7" type="operation">调集</Denoter>
					<Object oid="o7" sid="s8-s9">数十辆消防车</Object>
				</Event>
				<Event eid="e8">
					<Denoter did="d8" type="movement">赶赴</Denoter>
					<Location lid="l8" type="destination">现场</Location>
				</Event>，
				<Event eid="e9">进行
					<Denoter did="d9" type="operation">内攻灭火</Denoter>
				</Event>，
				<Event eid="e10" polarity="negative">
					<Time tid="t10" type="relTime">目前</Time>暂未发现
					<Participant sid="s10">人员</Participant>
					<Denoter did="d10" type="stateChange">被困</Denoter>
				</Event>。
			</Sentence>
			<Sentence>
				<Event eid="e11">下午2点左右，
					<Object sid="s11">大火</Object>基本
					<Denoter did="d11" type="stateChange">被控制</Denoter>
				</Event>。
			</Sentence>
		</Paragraph>
	</Content>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e11"/>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e2" thoughtcontent_eids="e3-e11"/>  
	<eRelation relType="Accompany" accompanya_eid="e7" accompanyb_eid="e8"/>  
	<eRelation relType="Follow" bevent_eid="e8" aevent_eid="e9"/> 
</Body>
