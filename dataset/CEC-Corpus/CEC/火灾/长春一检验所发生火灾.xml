<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>长春一检验所发生火灾</Title>  
	<ReportTime type="absTime">2014年03月31日 星期一</ReportTime>  
	<Content>
		<Paragraph> 
			<Sentence>
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">城市晚报</Participant>
					<Denoter type="statement" did="d1">讯</Denoter>
				</Event>  
				<Event eid="e2">
					<Time type="relTime" tid="t2">昨日15时许</Time>，
					<Location lid="l2">位于仙台大街与湛江路交会处的吉林省食品药品检验所</Location>发生
					<Denoter type="emergency" did="d2">火灾</Denoter>
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e3">
					<Denoter type="action" did="d3">接到</Denoter>
					<Object oid="o3">报警</Object>
				</Event>后， 
				<Event eid="e4">
					<Participant sid="s4">十多辆消防车</Participant>先后
					<Denoter type="movement" did="d4">赶到</Denoter>
					<Location type="destination" lid="l4">现场</Location>
				</Event> 进行
				<Event eid="e5">
					<Denoter type="operation" did="d5">救援</Denoter>
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event type="thoughtevent" eid="e6">据
					<Denoter type="statement" did="d6">悉</Denoter>
				</Event>， 
				<Event eid="e7">
					<Denoter type="emergency" did="d7">事故</Denoter>
				</Event>  
				<Event polarity="negative" eid="e8">未造成
					<Participant sid="s8">人员</Participant>
					<Denoter type="stateChange" did="d8">伤亡</Denoter>
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event type="thoughtevent" eid="e9">据
					<Location lid="l9">现场附近</Location>的
					<Participant sid="s9">围观群众</Participant>
					<Denoter type="statement" did="d9">称</Denoter>
				</Event>， 
				<Event eid="e10">
					<Denoter type="emergency" did="d10">事故</Denoter>约发生
					<Time type="relTime" tid="t10">在15时15分</Time>
				</Event>，起火点位于检验所西北角， 
				<Event eid="e11">
					<Time type="relTime" tid="t11">当时</Time>，不断有
					<Object sid="s11">滚滚浓烟</Object>从中
					<Denoter type="action" did="d11">冒出</Denoter>
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e12">
					<Time type="relTime" tid="t12">15时40分</Time>，当
					<Participant sid="s12">记者</Participant>
					<Denoter type="movement" did="d12">到达</Denoter>
					<Location type="destination" lid="l12">现场</Location>时
				</Event>， 
				<Event eid="e13">
					<Object oid="o13">火势</Object>已被
					<Denoter type="operation" did="d13">控制</Denoter>
				</Event>， 
				<Event eid="e14">
					<Participant sid="s14">消防人员</Participant>开始进入
					<Location lid="l14">建筑内部</Location>
					<Denoter type="operation" did="d14">施救</Denoter>。
				</Event>
			</Sentence>  
			<Sentence>
				<Event eid="e15">
					<Time type="relTime" tid="t15">16时许</Time>，
					<Object oid="o15">大火</Object>被
					<Denoter type="stateChange" did="d15">扑灭</Denoter>。
				</Event>
			</Sentence>  
			<Sentence>
				<Event eid="e16">
					<Time type="relTime" tid="t16">目前</Time>，
					<Object oid="o16">起火原因</Object>正在
					<Denoter type="operation" did="d16">调查</Denoter>之中
				</Event>。
			</Sentence>
		</Paragraph>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e16"/>  
		<eRelation relType="Follow" bevent_eid="e3" aevent_eid="e4"/>  
		<eRelation relType="Follow" bevent_eid="e4" aevent_eid="e5"/>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e6" thoughtcontent_eids="e8"/>  
		<eRelation relType="Thoughtcontent" thoughtevent_eid="e9" thoughtcontent_eids="e10-e16"/>  
		<eRelation relType="Accompany" accompanya_eid="e10" accompanyb_eid="e11"/>  
		<eRelation relType="Accompany" accompanya_eid="e12" accompanyb_eid="e13"/> 
	</Content>
</Body>
