		<eRelation relType="Follow" bevent_eid="e7" aevent_eid="e8"/>  
		<eRelation relType="Accompany" accompanya_eid="e8" accompanyb_eid="e9"/>  
		<eRelation relType="Follow" bevent_eid="e9" aevent_eid="e10"/>  
		<eRelation relType="Accompany" accompanya_eid="e11" accompanyb_eid="e12"/>  
		<eRelation relType="Accompany" accompanya_eid="e13" accompanyb_eid="e14"/>  
		<eRelation relType="Accompany" accompanya_eid="e14" accompanyb_eid="e15"/>  
		<eRelation relType="Follow" bevent_eid="e16" aevent_eid="e17"/>  
		<eRelation relType="Accompany" accompanya_eid="e18" accompanyb_eid="e19"/> 
	</Content>
