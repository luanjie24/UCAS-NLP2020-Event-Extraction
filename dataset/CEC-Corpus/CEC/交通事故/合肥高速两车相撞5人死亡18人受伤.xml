<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>合肥高速两车相撞5人死亡18人受伤</Title>  
	<ReportTime type="absTime">2007-10-8</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event eid="e1">
					<Participant sid="s1">新华网合肥</Participant>
					<Time type="relTime" tid="t1">10月8日</Time>
					<Denoter type="statement" did="d1">电</Denoter>
				</Event>（记者 程士华） 
				<Event eid="e2">记者从合肥市交警部门获悉， 
					<Time type="relTime" tid="t2">8日凌晨</Time>， 
					<Location lid="l2">合（合肥）宁（南京）高速公路</Location>上 
					<Object oid="o2">一辆货车</Object>与 
					<Object oid="o2">一辆大客车</Object>  
					<Denoter type="action" did="d2">相撞</Denoter> 
				</Event>，截至 
				<Event eid="e3"> 
					<Participant sid="s3">记者</Participant>  
					<Denoter type="action" did="d3">发稿</Denoter> 
				</Event>时， 
				<Event eid="e4">这起事故已造成 
					<Participant oid="o4">5人</Participant>  
					<Denoter type="stateChange" did="d4">死亡</Denoter> 
				</Event>， 
				<Event eid="e5"> 
					<Participant oid="o5">18人</Participant>  
					<Denoter type="stateChange" did="d5">受伤</Denoter> 
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e6">据处理事故的 
					<Participant sid="s6">合肥市交警支队高速公路一大队交警</Participant>  
					<Denoter type="statement" did="d6">介绍</Denoter> 
				</Event>， 
				<Event eid="e7"> 
					<Time type="relTime" tid="t7">10月8日凌晨2时许</Time>，一辆挂着江苏牌照的 
					<Object sid="s7">大货车</Object>，在 
					<Denoter type="action" did="d7">行经</Denoter>  
					<Location lid="l7">合宁高速公路（合肥－南京）15ＫＭ＋600Ｍ处</Location> 
				</Event>时， 
				<Event eid="e8">与来自河南省信阳市的 
					<Object oid="o8">一辆大客车</Object>  
					<Denoter type="action" did="d8">相撞</Denoter> 
				</Event>。
			</Sentence>  
			<Sentence>事故发生地点位于合肥市境内， 
				<Event eid="e9"> 
					<Time type="relTime" tid="t9">目前</Time>已造成 
					<Participant oid="o9">5人</Participant>  
					<Denoter type="stateChange" did="d9">死亡</Denoter> 
				</Event>， 
				<Event eid="e10"> 
					<Participant oid="o10">18人</Participant>不同程度 
					<Denoter type="stateChange" did="d10">受伤</Denoter> 
				</Event>。
			</Sentence> 
		</Paragraph>  
		<Paragraph> 
			<Sentence> 
				<Event eid="e11"> 
					<Participant sid="s11">合肥市委</Participant>、 
					<Participant sid="s11">市政府</Participant>高度 
					<Denoter type="stateChange" did="d11">重视</Denoter> 
				</Event>， 
				<Event eid="e12"> 
					<Participant sid="s12">市政府分管领导率市公安、安监、卫生等部门主要负责人</Participant>于 
					<Time type="relTime" tid="t12">第一时间</Time>  
					<Denoter type="movement" did="d12">赶赴</Denoter>  
					<Location lid="l12">现场</Location> 
				</Event>， 
				<Event eid="e13"> 
					<Denoter type="operation" did="d13">部署</Denoter>指挥救援 
					<Object oid="o13">工作</Object> 
				</Event>， 
				<Event eid="e14">并 
					<Location lid="l14">到医院</Location>  
					<Denoter type="operation" did="d14">看望</Denoter>  
					<Participant oid="o14">事故中受伤人员</Participant> 
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e15">同时要求 
					<Participant sid="s15,s16,s17">市有关部门</Participant>全力 
					<Denoter type="operation" did="d15">救治</Denoter>  
					<Participant oid="o15">受伤人员</Participant> 
				</Event>， 
				<Event eid="e16">立即开始 
					<Object oid="o16">事故</Object>  
					<Denoter type="operation" did="d16">调查</Denoter> 
				</Event>， 
				<Event eid="e17">认真 
					<Denoter type="operation" did="d17">做好</Denoter>  
					<Object oid="o17">善后工作</Object>。
				</Event> 
			</Sentence> 
		</Paragraph> 
	</Content>  
	<eRelation relType="Causal" cause_eid="e2" effect_eid="e4"/>  
	<eRelation relType="Causal" cause_eid="e2" effect_eid="e5"/>  
