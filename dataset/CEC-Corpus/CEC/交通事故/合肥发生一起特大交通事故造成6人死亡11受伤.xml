	<eRelation relType="Causal" cause_eid="e17" effect_eid="e19"/>  
	<eRelation relType="Causal" cause_eid="e17" effect_eid="e20"/>  
	<eRelation relType="Causal" cause_eid="e17" effect_eid="e21"/>  
	<eRelation relType="Causal" cause_eid="e25" effect_eid="e26"/> 
</Body>
