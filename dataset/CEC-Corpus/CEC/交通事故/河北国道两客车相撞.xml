	<ReportTime type="absTime">2007-8-21</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">晨报河北</Participant>
					<Denoter type="statement" did="d1">专讯</Denoter>
				</Event>  
				<Event eid="e2"> 
					<Time type="relTime" tid="t2">８月２１日７时４０分左右</Time>，在 
					<Location lid="l2">３０７国道河北省黄骅市一个路口</Location>， 
					<Object oid="o2">两辆客车</Object>  
