	<eRelation relType="Causal" cause_eid="e3" effect_eid="e4"/>  
	<eRelation relType="Follow" bevent_eid="e4" aevent_eid="e5"/>  
	<eRelation relType="Causal" cause_eid="e5" effect_eid="e6"/>  
	<eRelation relType="Causal" cause_eid="e5" effect_eid="e7"/> 
