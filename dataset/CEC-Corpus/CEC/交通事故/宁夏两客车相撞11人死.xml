<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>宁夏两客车相撞11人死</Title>  
	<ReportTime type="absTime">2006-8-29 8:43:17</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event type="thoughtevent" eid="e1"> 
					<Participant sid="s1">新华社</Participant>  
					<Denoter did="d1" type="statement">发</Denoter> 
				</Event> 
			</Sentence>  
			<Sentence> 
				<Event eid="e2">
					<Time type="relTime" tid="t2">当日中午12时17分</Time>， 在
					<Location lid="l2">211国道宁夏盐池县惠安堡段</Location>， 
					<Object sid="s2">两辆大客车</Object>发生迎面
					<Denoter type="action" did="d2">相撞</Denoter>事故。
				</Event> 
			</Sentence>  
			<Sentence>事故造成两车至少 
				<Event eid="e3">
					<Participant sid="s3">11人</Participant>  
					<Denoter type="stateChange" did="d3">死亡</Denoter>
				</Event>， 
				<Event eid="e4">
					<Participant sid="s4">25人</Participant>  
					<Denoter type="stateChange" did="d4">受伤</Denoter>
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e5">
					<Time type="relTime" tid="t5">目前</Time>  
					<Object oid="o5">事故原因</Object> 还在
					<Denoter type="operation" did="d5">调查</Denoter>之中
				</Event> 
			</Sentence>。 
			<Sentence>
				<Event eid="e6">
					<Denoter type="emergency" did="d6">事故</Denoter>发生
				</Event>后， 
				<Event eid="e7">宁夏自治区副主席
					<Participant sid="s7，s8，s9，s10">齐同生</Participant>已经 
					<Denoter type="operation" did="d7">带领</Denoter>有关部门的
					<Participant oid="o7" sid="s8，s9，s10">负责同志</Participant>
				</Event>  
				<Event eid="e8">
					<Denoter type="movement" did="d8">赶赴</Denoter>  
					<Location lid="l8">现场</Location>
				</Event>， 
				<Event eid="e9">
					<Denoter type="operation" did="d9">指挥</Denoter>救治伤员
				</Event>， 
				<Event eid="e10">
					<Denoter type="operation" did="d10">处理</Denoter>
					<Object oid="o10">善后事宜</Object>
				</Event>。
			</Sentence> 
		</Paragraph> 
	</Content>  
