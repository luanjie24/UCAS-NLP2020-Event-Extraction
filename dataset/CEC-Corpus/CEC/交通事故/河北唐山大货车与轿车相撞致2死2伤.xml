	<eRelation relType="Causal" cause_eid="e16" effect_eid="e18"/>  
	<eRelation relType="Follow" bevent_eid="e19" aevent_eid="e18"/>  
	<eRelation relType="Accompany" accompanya_eid="e19" accompanyb_eid="e20"/> 
</Body>
