<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>浙江甬台温高速货车追尾起大火致2人死亡</Title>  
	<ReportTime type="absTime">2007-4-1</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence>
				<Event eid="e1" type="thoughtevent">
					<Participant sid="s1">中新网</Participant>
					<Location lid="l1">温州</Location>
					<Time tid="t1" type="relTime">4月1日</Time>
					<Denoter did="d1" type="statement">电</Denoter>
				</Event>(江耘 徐行号 郑国前)
			</Sentence>  
			<Sentence>
				<Event eid="e2">
					<Time type="relTime" tid="t2">今日凌晨5点</Time>，
					<Location lid="l2">浙江甬台温高速公路温州段</Location>发生一起重大
					<Denoter type="emergency" did="d2">交通事故</Denoter>。
				</Event>
			</Sentence>  
			<Sentence>
				<Event eid="e3">
					<Object sid="s3">一辆半挂车</Object>与另
					<Object sid="s3">一辆抛锚货车</Object>发生
					<Denoter type="emergency" did="d3">追尾</Denoter>
				</Event>  
				<Event eid="e4">并引发
					<Denoter type="emergency" did="d4">大火</Denoter>
				</Event>，
				<Event eid="e5">造成
					<Participant sid="s5">2人</Participant>
					<Denoter type="stateChange" did="d5">死亡</Denoter>
				</Event>，
				<Event eid="e6">
					<Participant sid="s6">2人</Participant>
					<Denoter type="stateChange" did="d6">轻伤</Denoter>
				</Event>，
				<Event eid="e7">
					<Object sid="s7">2辆挂车</Object>
					<Denoter type="stateChange" did="d7">报废</Denoter>
				</Event>，同时还
				<Event eid="e8">
					<Denoter type="stateChange" did="d8">烧毁</Denoter>了
					<Object oid="o8">10余吨布料</Object>
				</Event>。
			</Sentence>
		</Paragraph> 
	</Content>  
	<eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e8"/>  
	<eRelation relType="Causal" cause_eid="e3" effect_eid="e4"/>  
	<eRelation relType="Causal" cause_eid="e4" effect_eid="e5"/>  
	<eRelation relType="Causal" cause_eid="e4" effect_eid="e6"/>  
	<eRelation relType="Causal" cause_eid="e4" effect_eid="e7"/>  
	<eRelation relType="Causal" cause_eid="e4" effect_eid="e8"/> 
</Body>
