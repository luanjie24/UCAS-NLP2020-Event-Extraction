<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>杭州一槽罐车压扁小轿车 小车司机当场丧生</Title>  
	<ReportTime type="absTime">2008-8-8 20:00:01</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event type="thoughtevent" eid="e1">
					<Participant sid="s1">本报</Participant>
					<Denoter type="statement" did="d1">讯</Denoter>
				</Event>  
				<Event eid="e2"> 
					<Time type="relTime" tid="t2">昨天中午12点左右</Time>， 
					<Location lid="l2">杭州市文一西路荆大线口</Location>， 
					<Object oid="o2">一声巨响</Object>  
					<Denoter type="action" did="d2">传来</Denoter> 
				</Event>， 
				<Event eid="e3">正在附近开店的 
					<Participant sid="s3">王师傅</Participant>一 
					<Denoter type="perception" did="d3">看</Denoter> 
				</Event>， 
				<Event eid="e4"> 
					<Object sid="s4">一辆槽罐车</Object>把一辆黑色的 
					<Object oid="o4">本田小轿车</Object>  
					<Denoter type="action" did="d4">压</Denoter>在身下了。
				</Event> 
			</Sentence> 
		</Paragraph>  
		<Paragraph> 
			<Sentence> 
				<Event eid="e5"> 
					<Time type="relTime" tid="t5">随后</Time>， 
					<Participant sid="s5">交警</Participant>及 
					<Object sid="s5">施救车辆</Object>  
					<Denoter type="movement" did="d5">赶到</Denoter>  
					<Location type="destination" lid="l5">现场</Location> 
				</Event>。
			</Sentence>
			<Sentence> 
				<Event eid="e6">当 
					<Participant sid="s6">大家</Participant>把 
					<Object oid="o6">小轿车</Object>从大车底下 
					<Denoter type="action" did="d6">拖</Denoter>出来时
				</Event>， 
				<Event eid="e7">整辆 
					<Object oid="o7">车</Object>已经 
					<Denoter type="stateChange" did="d7">变形</Denoter>了
				</Event>， 
				<Event eid="e8">车内的 
					<Participant oid="o8">驾驶员</Participant>当场 
					<Denoter type="stateChange" did="d8">死亡</Denoter> 
				</Event>。
			</Sentence> 
		</Paragraph> 
	</Content>  
	<eRelation relType="Follow" bevent_eid="e3" aevent_eid="e2"/>  
	<eRelation relType="Causal" cause_eid="e4" effect_eid="e7"/>  
	<eRelation relType="Follow" bevent_eid="e6" aevent_eid="e5"/>  
	<eRelation relType="Accompany" accompanya_eid="e7" accompanyb_eid="e8"/> 
